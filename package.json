{
  "name": "fswd-project-seed",
  "version": "1.0.0",
  "description": "Project seed for Intro to Full Stack Web Development",
  "main": "lib/server.js",
  "scripts": {
    "test": "npm run karma && npm run mocha",
    "start": "nodemon ./lib/server.js",
    "dev": "NODE_ENV=development nodemon ./lib/server.js",
    "dev_db_create": "createdb fswd_seed_development && sequelize db:migrate",
    "dev_db_connect": "psql fswd_seed_development",
    "test_db_create": "createdb fswd_seed_test",
    "sequelize": "sequelize",
    "postinstall": "jspm install",
    "karma": "karma start",
    "mocha": "istanbul cover -x '**/models/index.js' _mocha -- -R spec test/server/*-test.js",
    "webdriver": "webdriver-manager update && webdriver-manager start",
    "test-server": "NODE_ENV=test PORT=8008 nodemon ./lib/server.js",
    "protractor-backend": "npm run webdriver & npm run test-server",
    "protractor": "protractor ./protractor.config.js",

    "lint": "eslint ."
  },
  "keywords": [
    "fswd",
    "betamore"
  ],
  "author": "David Raynes",
  "license": "ISC",
  "devDependencies": {
    "chai": "^3.5.0",
    "eslint": "^3.1.1",
    "eslint-plugin-angular": "^2.2.0",
    "express-debug": "^1.1.1",
    "jspm": "^0.16.39",
    "karma": "^1.6.0",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^1.1.1",
    "karma-jspm": "^2.2.0",
    "karma-mocha": "^1.1.1",
    "karma-phantomjs-launcher": "^1.0.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.27",
    "mocha": "^3.1.2",
    "morgan": "^1.7.0",
    "supertest": "^3.0.0",
    "supertest-as-promised": "^4.0.2"
  },
  "dependencies": {
    "bcrypt": "^1.0.0",
    "body-parser": "^1.17.1",
    "connect-redis": "^3.1.0",
    "cookie-parser": "^1.4.3",
    "express": "^4.14.0",
    "express-session": "^1.14.0",
    "jade": "^1.11.0",
    "lodash": "^4.13.1",
    "nodemon": "^1.9.2",
    "pg": "^6.0.3",
    "sequelize": "^3.23.6",
    "sequelize-cli": "^2.4.0"
  },
  "jspm": {
    "directories": {
      "baseURL": "public"
    },
    "dependencies": {
      "angular": "github:angular/bower-angular@^1.5.8",
      "angular-route": "github:angular/bower-angular-route@^1.5.8",
      "lodash": "npm:lodash@^4.13.1"
    },
    "devDependencies": {
      "angular-mocks": "github:angular/bower-angular-mocks@^1.5.8"
    }
  }
}
